<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.activities.HouseDeviceGarageActivity">

    <Button
        android:id="@+id/buttonUpGarage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onButtonUpGarageClic"
        android:text="ouvir"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="99dp" />

    <Button
        android:id="@+id/buttonDownGarage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onButtonDownGarageClic"
        android:text="fermer"
        app:layout_constraintTop_toBottomOf="@+id/buttonUpGarage"
        tools:layout_editor_absoluteX="107dp" />

    <Button
        android:id="@+id/buttonStopGarage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onButtonStopGarageClic"
        android:text="stop"
        app:layout_constraintTop_toBottomOf="@+id/buttonDownGarage"
        tools:layout_editor_absoluteX="105dp" />
</androidx.constraintlayout.widget.ConstraintLayout>